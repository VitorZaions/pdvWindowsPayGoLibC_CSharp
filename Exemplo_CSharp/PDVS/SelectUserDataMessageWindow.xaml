            <ComboBoxItem Name="cbMin2">2</ComboBoxItem>
            <ComboBoxItem Name="cbMin3">3</ComboBoxItem>
            <ComboBoxItem Name="cbMin4">4</ComboBoxItem>
            <ComboBoxItem Name="cbMin5">5</ComboBoxItem>
            <ComboBoxItem Name="cbMin6">6</ComboBoxItem>
            <ComboBoxItem Name="cbMin7">7</ComboBoxItem>
            <ComboBoxItem Name="cbMin8">8</ComboBoxItem>
            <ComboBoxItem Name="cbMin9">9</ComboBoxItem>
            <ComboBoxItem Name="cbMin10">10</ComboBoxItem>
            <ComboBoxItem Name="cbMin11">11</ComboBoxItem>
            <ComboBoxItem Name="cbMin12">12</ComboBoxItem>
        </ComboBox>
        <ComboBox x:Name="cmbQtdMax" HorizontalAlignment="Left" Margin="286,41,0,0" VerticalAlignment="Top" Width="56">
            <ComboBoxItem Name="cbMax1">1</ComboBoxItem>
            <ComboBoxItem Name="cbMax2">2</ComboBoxItem>
            <ComboBoxItem Name="cbMax3">3</ComboBoxItem>
            <ComboBoxItem Name="cbMax4">4</ComboBoxItem>
            <ComboBoxItem Name="cbMax5">5</ComboBoxItem>
            <ComboBoxItem Name="cbMax6">6</ComboBoxItem>
            <ComboBoxItem Name="cbMax7">7</ComboBoxItem>
            <ComboBoxItem Name="cbMax8">8</ComboBoxItem>
            <ComboBoxItem Name="cbMax9">9</ComboBoxItem>
            <ComboBoxItem Name="cbMax10">10</ComboBoxItem>
            <ComboBoxItem Name="cbMax11">11</ComboBoxItem>
            <ComboBoxItem Name="cbMax12">12</ComboBoxItem>
        </ComboBox>

